                    <Button fx:id="searchButton" id="buttonSearch" mnemonicParsing="false" onMouseClicked="#searchBtnAct"
                            onMouseEntered="#searchBtnActFocus"
                            onKeyPressed="#handleOnKeyPressedSearch" prefHeight="50.0" prefWidth="300.0"
                            text="SEARCH">
                        <VBox.margin>
                            <Insets left="50.0" right="25.0"/>
                        </VBox.margin>
                        <graphic>
                            <ImageView fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true">
                                <image>
                                    <Image url="Duelyst/css/searchWhite.png"/>
                                </image>
                            </ImageView>
                        </graphic>
                    </Button>
                </children>
            </VBox>
        </children>
    </GridPane>
</StackPane>
